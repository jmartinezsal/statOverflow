extends layout.pug
block append head 
block content
  img(src="/images/title.png" class="title__image")
  div.users__container
    h1= title
    #all__users__container
      for user in users
        .user__container
          a(href=`/users/${user.id}`)
            image(src=`${user.avatarImage}`)
            br
            p= user.username
